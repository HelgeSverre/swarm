# OpenAI Configuration
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL="gpt-4.1-mini"
OPENAI_TEMPERATURE=0.7

# Logging Configuration
LOG_ENABLED=true
LOG_PATH=logs
LOG_LEVEL=info
APP_ENV=local

# Timeout Configuration
SWARM_REQUEST_TIMEOUT=600         # Per-request timeout in seconds (default: 600 = 10 minutes)
SWARM_SUBPROCESS_TIMEOUT=300      # Subprocess timeout in seconds (default: 300 = 5 minutes)
SWARM_HEARTBEAT_INTERVAL=30       # Heartbeat interval in seconds (default: 30)
SWARM_TIMEOUT_RETRY_ENABLED=true  # Enable retry suggestion on timeout (default: true)